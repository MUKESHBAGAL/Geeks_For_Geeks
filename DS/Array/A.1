###C++

class Solution {
  public:
    int search(vector<int>& arr, int x) {
        for(int i=0;i<arr.size();i++){
            if(arr[i]==x)
                return i;
        }
        return -1;
    }
};


###JAVA

class Solution {

    static int search(int arr[], int x) {
        for(int i=0;i<arr.length;i++){
            if(arr[i]==x)
                return i;
        }
        return -1;
    }
}

###PYTHON
class Solution:
    def search(self, arr, x):
        if x in arr:
            return arr.index(x)
        else:
            return -1

