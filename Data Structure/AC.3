###C++

class Solution {
public:
    vector<int> find(vector<int> arr, int x) {
        vector<int> ans(2, -1);
        for (int i = 0; i < arr.size(); i++) {
            if (arr[i] == x) {
                if (ans[0] == -1) {
                    ans[0] = i;
                }
                ans[1] = i;
            }
        }
        return ans;
    }
};


