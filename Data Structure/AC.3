###PYTHON

class Solution:
    def find(self, arr, x):
        ans = [-1, -1]
        
        for i in range(len(arr)):
            if arr[i] == x:
                if ans[0] == -1:
                    ans[0] = i
                ans[1] = i
                
        return ans

