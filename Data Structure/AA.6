###C++

class Solution {
  public:
    // arr[] is the array
    void print(vector<int> &arr) {
        for (int i = 0; i < arr.size(); i += 2) {
            cout<<arr[i]<<" ";
        }
    }
};


###JAVA

class Solution {
    public static void print(int arr[]) {
       for (int i = 0; i < arr.length; i += 2) {
            System.out.print(arr[i] + " ");
        }
    }
}

