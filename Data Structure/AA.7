class Solution {
    void immediateSmaller(int arr[], int n) {
      int len = arr.length;
        int[] nums = new int[len];
        
        for (int i = 0; i < len - 1; i++) {
            if (arr[i] > arr[i + 1]) {
                nums[i] = arr[i + 1];
            } else {
                nums[i] = -1;
            }
        }
        nums[len - 1] = -1;
        for (int i=0;i<len;i++){
            arr[i]=nums[i];
        }
    }
}

