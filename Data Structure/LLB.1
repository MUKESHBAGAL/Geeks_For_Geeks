IN C++

class Solution {
  public:
    int getKthFromLast(Node *head, int k) {
        struct Node *temp=head;
        struct Node *ptr=head;
        int c=0;
        if(temp==NULL)
        return 0;
        
        while(temp!=NULL){
            temp=temp->next;
            c++;
        }
        if(k>c)
            return -1;
        else{
            for(int i=0;i<abs(k-c);i++)
            ptr=ptr->next;
        }
        
        return ptr->data;
    }
};
