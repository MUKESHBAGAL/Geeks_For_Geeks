###IN C++

vector<int> displayList(Node *head) {
    Node *temp = head;
    vector<int> ans;
    while (temp) {
        ans.push_back(temp->data);
        temp = temp->next;
    }
    return ans;
}


###IN JAVA

class Get {
    public static ArrayList<Integer> displayList(Node head) {
        ArrayList<Integer> ans = new ArrayList<>();
        Node temp = head;
        while (temp != null) {
            ans.add(temp.data);
            temp = temp.next;
        }
        return ans;
    }
}

