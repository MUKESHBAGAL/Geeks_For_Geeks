###IN C++

class Solution {
  public:
    int findTarget(vector<int>& arr, int target) {
        for (int i = 0; i < arr.size(); i++) {
            if (arr[i] == target)
                return i;
        }
        return -1;
    }
};

###IN JAVA

class Solution {
    public int findTarget(int arr[], int target) {
        for (int i = 0; i < arr.length; i++) {
            if (arr[i] == target)
                return i;
        }
        return -1;
        
    }
}


###IN PYTHON3

class Solution:
    def findTarget(self, arr, target):
        if target in arr:
            return arr.index(target)
        return -1
        
