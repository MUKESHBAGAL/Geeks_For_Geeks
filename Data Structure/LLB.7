IN c++

class Solution
{
    public:
    Node* findIntersection(Node* head1, Node* head2)
    {
        Node* temp1 = head1;
        Node* temp2 = head2;
        Node* head = NULL;
        Node* tail = NULL;

        while (temp1 != NULL && temp2 != NULL) {
            if (temp1->data == temp2->data) {
                Node* newNode = new Node(temp1->data);
                
                if (head == NULL) {
                    head = tail = newNode;
                } else {
                    tail->next = newNode;
                    tail = tail->next;
                }

                temp1 = temp1->next;
                temp2 = temp2->next;
            } else if (temp1->data < temp2->data) {
                temp1 = temp1->next;
            } else {
                temp2 = temp2->next;
            }
        }
        
        return head;
    }
};

