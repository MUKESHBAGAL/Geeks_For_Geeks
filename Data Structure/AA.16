###C++

class Solution {
public:
    int lastIndex(string s) {
        for (int i = s.length() - 1; i >= 0; i--) {
            if (s[i] == '1') {
                return i;
            }
        }
        return -1;
    }
};

###JAVA
class Solution {
    public int lastIndex(String s) {
        
        return s.lastIndexOf('1');
    }
}
