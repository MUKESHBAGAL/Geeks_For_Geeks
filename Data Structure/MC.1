###IN C

bool searchMatrix(int** mat, int n, int m, int x) {
    for (int i = 0; i < n; i++) {
        for (int j = 0; j< m; j++) {
            if (mat[i][j] == x) {
                return true;
            }
        }
    }
    return false;
    
}


###IN C++

class Solution {
  public:
    // Function to search a given number in row-column sorted matrix.
    bool searchMatrix(vector<vector<int>> &mat, int x) {
        for (const auto& nums : mat) {
            if (find(nums.begin(), nums.end(), x) != nums.end()) {
                return true;
            }
        }
        return false;

        
    }
};



###IN JAVA


class Solution {
    // Function to search a given number in row-column sorted matrix.
    public boolean searchMatrix(int[][] mat, int x) {
        for (int[] nums : mat) {
            for (int num : nums) {
                if (num == x) {
                    return true;
                }
            }
        }
        return false;
        
    }
}



