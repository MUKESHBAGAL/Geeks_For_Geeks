### IN C

void Addition(int n, int matrixA[][n], int matrixB[][n]) {
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                matrixA[i][j] = matrixA[i][j] + matrixB[i][j];
            }
        }
}


###IN C++

class Solution {
  public:
    void Addition(vector<vector<int>>& matrixA, vector<vector<int>>& matrixB) {
        // Code here
        int n = matrixA.size();
       

        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                matrixA[i][j] = matrixA[i][j] + matrixB[i][j];
            }
        }
    }
};


###IN JAVA


// User function Template for Java

class Solution {
    public void Addition(int[][] matrixA, int[][] matrixB) {
        int n = matrixA.length;

        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                matrixA[i][j] = matrixA[i][j] + matrixB[i][j];
            }
        }
        
    }
}




    

