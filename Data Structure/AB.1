### C++

class Solution {
  public:
    // Function returns the second
    // largest elements
    int print2largest(vector<int> &arr) {
       int first_max=arr[0],sec_max;
       for(int i=1;i<arr.size();i++){
           if(arr[i]>first_max){
               sec_max=first_max;
               first_max=arr[i];
           }
           else if (arr[i] > sec_max && arr[i] < first_max) 
                sec_max = arr[i];
       }
       return sec_max;
    }
};

### JAVA

class Solution {
    public int print2largest(int[] arr) {
        int max,second_max=0;
        max=arr[0];
        for(int i=1;i<arr.length;i++){
            if(arr[i]>max){
                second_max=max;
                max=arr[i];
            }
            if(second_max<arr[i] && max>arr[i])
                second_max=arr[i];
        }
        return second_max;
    }
}
