###IN C++

class Solution {
  public:
    int sumOfMatrix(int N, int M, vector<vector<int>> Grid) {
        int totalSum = 0;
        for (int i = 0; i < N; ++i) {
            for (int j = 0; j < M; ++j) {
                totalSum += Grid[i][j];
            }
        }
        return totalSum;
        
    }
};



