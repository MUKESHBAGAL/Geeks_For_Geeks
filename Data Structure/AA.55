###C++

class Solution {
    int findSum(int arr[]) {
        Set<Integer> result = new TreeSet<>();
        for (int num : arr) {
            result.add(num);
        }
        int sum = 0;
        for (int num : result) {
            sum += num;
        }
        return sum;
        
    }
}



