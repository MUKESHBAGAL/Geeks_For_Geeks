####IN C++

class Solution {
  public:
    string removeDuplicates(string &s) {
        unordered_set<char> seen;
        string ans = "";

        for (char ch : s) {
            if (seen.find(ch) == seen.end()) {
                seen.insert(ch);
                ans += ch;
            }
        }
        return ans;
        
    }
};

####IN JAVA

class Solution {
    String removeDuplicates(String s) {
        HashSet<Character> seen = new HashSet<>();
        StringBuilder ans = new StringBuilder();

        for (char ch : s.toCharArray()) {
            if (!seen.contains(ch)) {
                seen.add(ch);
                ans.append(ch);
            }
        }
        return ans.toString();
    }
}


