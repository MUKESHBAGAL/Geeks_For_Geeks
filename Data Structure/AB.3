### C++
class Solution {
public:
    void pushZerosToEnd(std::vector<int>& arr) {
        int lastNonZeroFoundAt = 0;
        int n = arr.size();
        
        for (int i = 0; i < n; i++) {
            if (arr[i] != 0) {
                std::swap(arr[i], arr[lastNonZeroFoundAt]);
                lastNonZeroFoundAt++;
            }
        }
    }
};


### JAVA

class Solution {
    void pushZerosToEnd(int[] arr, int n) {
        int lastNonZeroFoundAt = 0;
        
        for (int i = 0; i < n; i++) {
            if (arr[i] != 0) {
                int temp = arr[i];
                arr[i] = arr[lastNonZeroFoundAt];
                arr[lastNonZeroFoundAt] = temp;
                
                lastNonZeroFoundAt++;
            }
        }
    }
}
